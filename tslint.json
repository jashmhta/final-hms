{
  "extends": [
    "tslint:latest",
    "tslint-config-prettier",
    "tslint-config-standard",
    "tslint-react",
    "tslint-immutable"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-implicit-dependencies": false,
    "no-submodule-imports": false,
    "no-var-requires": false,
    "ordered-imports": [
      true,
      {
        "grouped-imports": true,
        "groups": [
          {
            "name": "third-party",
            "match": "^[^./]",
            "order": 10
          },
          {
            "name": "first-party",
            "match": "^@/",
            "order": 20
          },
          {
            "name": "relative",
            "match": "^\\.",
            "order": 30
          }
        ]
      }
    ],
    "object-literal-sort-keys": false,
    "member-access": [
      true,
      "no-public"
    ],
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "static-method",
          "instance-method"
        ]
      }
    ],
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ],
    "max-line-length": [
      true,
      88
    ],
    "prefer-const": true,
    "no-console": [
      true,
      "log"
    ],
    "no-unused-variable": true,
    "no-empty": false,
    "no-shadowed-variable": true,
    "no-string-literal": false,
    "no-bitwise": false,
    "no-unused-expression": true,
    "prefer-for-of": true,
    "array-type": [
      true,
      "array"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "deprecation": true,
    "import-spacing": true,
    "newline-before-return": true,
    "no-boolean-literal-compare": true,
    "no-consecutive-blank-lines": [
      true,
      1
    ],
    "no-trailing-whitespace": true,
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-catch",
      "check-finally",
      "check-whitespace"
    ],
    "one-variable-per-declaration": true,
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      },
      {
        "call-signature": "onespace",
        "index-signature": "onespace",
        "parameter": "onespace",
        "property-declaration": "onespace",
        "variable-declaration": "onespace"
      }
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type",
      "check-typecast"
    ],
    "jsx-wrap-multiline": true,
    "jsx-alignment": true,
    "jsx-boolean-value": [
      true,
      "never"
    ],
    "jsx-curly-spacing": [
      true,
      "never"
    ],
    "jsx-equals-spacing": [
      true,
      "never"
    ],
    "jsx-key": true,
    "jsx-no-bind": true,
    "jsx-no-lambda": true,
    "jsx-no-multiline-js": false,
    "jsx-no-string-ref": true,
    "jsx-self-close": true,
    "jsx-space-before-trailing-slash": true,
    "readonly-keyword": true,
    "readonly-array": true,
    "no-let": true,
    "no-object-mutation": true,
    "no-delete": true,
    "no-method-signature": true,
    "no-eval": true,
    "no-implied-eval": true,
    "no-new-func": true,
    "no-dangerous-decorators": true,
    "no-unsafe-any": true,
    "cyclomatic-complexity": [
      true,
      10
    ],
    "max-classes-per-file": [
      true,
      1
    ],
    "max-file-line-count": [
      true,
      500
    ],
    "no-angle-bracket-type-assertion": true,
    "no-empty-interface": true,
    "no-floating-promises": true,
    "no-for-in-array": true,
    "no-import-side-effect": true,
    "no-invalid-template-strings": true,
    "no-invalid-this": true,
    "no-misused-new": true,
    "no-non-null-assertion": true,
    "no-string-throw": true,
    "no-unbound-method": true,
    "no-unnecessary-class": true,
    "no-unnecessary-initializer": true,
    "no-unnecessary-qualifier": true,
    "no-unnecessary-type-assertion": true,
    "no-unsafe-finally": true,
    "prefer-conditional-expression": true,
    "prefer-function-over-method": true,
    "prefer-object-spread": true,
    "prefer-readonly": true,
    "prefer-switch": true,
    "strict-boolean-expressions": true,
    "switch-default": true,
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typeof-compare": true,
    "use-default-type-parameter": true,
    "jsx-a11y/accessible-emoji": true,
    "jsx-a11y/alt-text": true,
    "jsx-a11y/anchor-has-content": true,
    "jsx-a11y/anchor-is-valid": true,
    "jsx-a11y/aria-activedescendant-has-tabindex": true,
    "jsx-a11y/aria-props": true,
    "jsx-a11y/aria-proptypes": true,
    "jsx-a11y/aria-role": true,
    "jsx-a11y/aria-unsupported-elements": true,
    "jsx-a11y/autocomplete-valid": true,
    "jsx-a11y/click-events-have-key-events": true,
    "jsx-a11y/heading-has-content": true,
    "jsx-a11y/html-has-lang": true,
    "jsx-a11y/img-redundant-alt": true,
    "jsx-a11y/interactive-supports-focus": true,
    "jsx-a11y/label-has-associated-control": true,
    "jsx-a11y/label-has-for": true,
    "jsx-a11y/lang": true,
    "jsx-a11y/mouse-events-have-key-events": true,
    "jsx-a11y/no-access-key": true,
    "jsx-a11y/no-autofocus": true,
    "jsx-a11y/no-distracting-elements": true,
    "jsx-a11y/no-interactive-element-to-noninteractive-role": true,
    "jsx-a11y/no-noninteractive-element-interactions": true,
    "jsx-a11y/no-noninteractive-element-to-interactive-role": true,
    "jsx-a11y/no-onchange": true,
    "jsx-a11y/no-redundant-roles": true,
    "jsx-a11y/role-has-required-aria-props": true,
    "jsx-a11y/role-supports-aria-props": true,
    "jsx-a11y/scope": true,
    "jsx-a11y/tabindex-no-positive": true,
    "completed-docs": [
      true,
      {
        "functions": {
          "visibilities": [
            "exported"
          ]
        },
        "classes": {
          "visibilities": [
            "exported"
          ]
        },
        "enums": {
          "visibilities": [
            "exported"
          ]
        },
        "interfaces": {
          "visibilities": [
            "exported"
          ]
        },
        "types": {
          "visibilities": [
            "exported"
          ]
        },
        "variables": {
          "kinds": [
            "const",
            "let"
          ],
          "visibilities": [
            "exported"
          ]
        }
      }
    ],
    "file-name-casing": [
      true,
      "camel-case"
    ],
    "linebreak-style": [
      true,
      "LF"
    ],
    "match-default-export-name": true,
    "no-circular-imports": true,
    "no-mergeable-namespace": true,
    "no-require-imports": true,
    "no-unused-imports": true
  },
  "rulesDirectory": [
    "node_modules/tslint-immutable/rules",
    "node_modules/tslint-react/rules",
    "node_modules/tslint-config-prettier/rules",
    "node_modules/tslint-config-standard/rules"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**",
      "dist/**",
      "build/**",
      "coverage/**",
      "**/*.json",
      "**/*.js",
      "**/*.test.ts",
      "**/*.test.tsx",
      "**/*.spec.ts",
      "**/*.spec.tsx"
    ]
  }
}